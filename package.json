  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@types/sinon": "^10.0.15",
    "hardhat": "^2.16.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2",
    "socket.io": "^4.7.1"
  }
}
