{
  "name": "hardhat",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx hardhat node",
    "gen": "node -r ts-node/register gen.ts AzukiDemo && node -r ts-node/register gen.ts NFTMarketPlace",
    "compile": "npx hardhat compile && npm run gen",
    "deploy:nft": "npx hardhat run scripts/AzukiDemo.deploy.ts --network",
